{
    "name": "Foblox",
    "description": "This Extension Lets you give insides of Roblox Experiences And Much More!",
    "version": "1.0.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "backgroundLoader.js",
      "type": "module"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": ["storage","scripting", "activeTab","tabs","cookies","unlimitedStorage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "19": "/images/icon_19.png",
            "128": "/images/icon_128.png",
            "150": "/images/icon_150.png"
          }
      },
      "icons": {
        "19": "/images/icon_19.png",
        "128": "/images/icon_128.png",
        "150": "/images/icon_150.png"
      },
      "options_page": "options.html"
  }